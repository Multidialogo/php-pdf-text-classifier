{
  "name": "multidialogo/php-pdf-text-classifier",
  "type": "library",
  "description": "A simple pdf text classifier",
  "keywords": ["pdf", "text", "classifier"],
  "homepage": "https://github.com/zio-mitch/php-pdf-text-classifier",
  "license": "MIT",
  "authors": [
    {
      "name": "Michele Carino",
      "email": "michelecarino84@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.3",
    "Smalot\\PdfParser\\Parser": "^2.11"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpunit/phpunit-prophecy": "^3.0"
  },
  "autoload": {
    "psr-4": { "Multidialogo\\PdfTextClassifier\\": "src" },
    "exclude-from-classmap": [
      "/src/Tests/"
    ]
  },
  "minimum-stability": "dev"
}